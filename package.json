{
  "name": "eslint-plugin-impress",
  "version": "2.3.0",
  "description": "ESLint plugin for compatibility with Impress Application Server",
  "main": "plugin.js",
  "scripts": {
    "test": "npm run -s lint",
    "lint": "eslint . && prettier -c \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\"",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aqrln/eslint-plugin-impress.git"
  },
  "keywords": [
    "eslint",
    "plugin",
    "ias",
    "impress",
    "metarhia"
  ],
  "author": "Alexey Orlenko <eaglexrlnk@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aqrln/eslint-plugin-impress/issues"
  },
  "homepage": "https://github.com/aqrln/eslint-plugin-impress#readme",
  "files": [],
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-metarhia": "^6.0.0",
    "eslint-config-prettier": "^3.5.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "1.16.x"
  },
  "peerDependencies": {
    "eslint": ">=3.17.0"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
